# RealVictors Environment Variables
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Stripe Configuration (for payments)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Mux Configuration (for video processing)
EXPO_PUBLIC_MUX_ENV_KEY=your_mux_environment_key_here
MUX_TOKEN_ID=your_mux_token_id_here
MUX_TOKEN_SECRET=your_mux_token_secret_here

# Google Maps API (for location services)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google OAuth (for social login)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Apple OAuth (for social login)
EXPO_PUBLIC_APPLE_CLIENT_ID=your_apple_client_id_here

# Expo Push Notifications
EXPO_PUSH_TOKEN=your_expo_push_token_here

# Sentry (for error tracking)
SENTRY_DSN=your_sentry_dsn_here
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_BASE_URL=https://your-supabase-url.supabase.co
EXPO_PUBLIC_APP_SCHEME=realvictors

# Feature Flags
EXPO_PUBLIC_ENABLE_BETTING=false
EXPO_PUBLIC_ENABLE_REFEREE_SYSTEM=true
EXPO_PUBLIC_ENABLE_VIDEO_CALLS=false
EXPO_PUBLIC_ENABLE_LIVE_STREAMING=false
